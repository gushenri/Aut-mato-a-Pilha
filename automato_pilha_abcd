digraph {
	q0 [shape=circle]
	q1 [shape=circle]
	q2 [shape=circle]
	q3 [shape=circle]
	qf [shape=doublecircle]
	q0 -> q0 [label="a; empilha A"]
	q0 -> q1 [label="b; transita"]
	q1 -> q1 [label="b; desempilha A"]
	q1 -> q2 [label="b; transita q2"]
	q2 -> q2 [label="c; empilha C"]
	q2 -> q3 [label="d; transita"]
	q3 -> q3 [label="d; desempilha C"]
	q3 -> qf [label="d; transita qf"]
	"" -> q0 [label="in√≠cio"]
}
